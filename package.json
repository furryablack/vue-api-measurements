{
  "name": "vue-api-measurements",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint ./",
    "cleanup": "rm -rf dist && mkdir dist",
    "build:indexes": "node scripts/build.js",
    "build:types": "vue-tsc --project ./tsconfig.build.json",
    "build:commonjs": "yarn babel --verbose --out-dir dist src -x .ts --ignore '**/*.d.ts'",
    "build:mjs": "ESMODULES=true yarn build:commonjs --out-file-extension .mjs",
    "build:bundles": "rollup -c",
    "build": "run-s cleanup build:indexes build:types build:commonjs build:mjs build:bundles"
  },
  "author": "Furrya Black <furrya.black@gmail.com>",
  "license": "MIT",
  "version": "0.1.1",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@eslint-kit/eslint-config-base": "^2.1.0",
    "@eslint-kit/eslint-config-node": "^2.0.0",
    "@eslint-kit/eslint-config-typescript": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@typescript-eslint/parser": "^4.2.0",
    "camel-case": "^4.1.1",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "^7.9.0",
    "globby": "^11.0.0",
    "husky": "3.1.0",
    "lint-staged": "^9.5.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.32.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.2.3",
    "vue": "^3.2.0",
    "vue-tsc": "^0.29.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
